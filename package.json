        "lodash": "^4.17",
        "resolve-url-loader": "^5.0",
        "sass": "^1.68",
        "sass-loader": "^12.0",
        "vue": "^2.7",
        "vue-template-compiler": "^2.7"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4",
        "@mdi/font": "^7.0",
        "v-money": "^0.9",
        "vee-validate": "^3.4",
        "vue-google-charts": "^0.4",
        "vue-qrcode-reader": "^3.0",
